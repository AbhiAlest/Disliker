{
  "manifest_version": 2,
  "name": "YouTube Dislike Counter",
  "version": "1.0", //2.0 beta extension
  "description": "Shows the dislike count for the current YouTube video",
  "icons": {
    "16": "icon16.png", //add 16 x 16 png
    "48": "icon48.png", //add 48 x 48 png
    "128": "icon128.png" //add 128 x 128 png
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
