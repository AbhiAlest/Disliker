{
  "manifest_version": 2,
  "name": "YouTube Dislike Counter",
  "version": "1.0",
  "description": "Shows the dislike count for the current YouTube video",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
